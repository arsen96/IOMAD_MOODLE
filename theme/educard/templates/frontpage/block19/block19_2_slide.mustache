<div id="block19-2-s" class="block19-2 educard-block block-start-end-space pt-40 pb-40 educard-font site-background-color">
    <div class="container">
        {{#block19headerenabled}}
        <div class="educard-sub-title text-white">
            <h6>{{block19headline}}</h6>
        </div>
        <div>
            <div class="h1">{{{block19header}}} <span class="text-white">{{block19maintitle}}</span></div>
            <div class="educard-heading-line-left bg-dark"></div>
        </div>
        {{/block19headerenabled}}
        <div class="pt-40 align-items-center ">
            <div id="block19-2-slider" class="splide block19-2-slider" >
                <div class="splide__track" >
                    <ul class="splide__list">
                        {{#block19}}
                        <li class="splide__slide">
                            <div class="text-center pt-10 pb-10">
                                <div class="card h-100" >
                                    <a class="text-decoration-none" href="{{{link}}}">
                                        {{#image19}}
                                        <img src="{{image19}}" alt="img">
                                        {{/image19}}
                                        {{^image19}}
                                        <img src="{{frontpageimglink}}block19/d2/{{count}}.jpg" alt="img" onerror="this.style.display='none'">
                                        {{/image19}}
                                    </a> 
                                </div>    
                            </div>
                        </li>
                        {{/block19}}
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div> 
