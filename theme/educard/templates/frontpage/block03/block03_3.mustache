<div id="block03-3" class="block03-3 educard-block educard-font">
    <div class="block03-bg block-start-end-space">
        <div class="container">
            <div class="educard-sub-title">
                <h6>{{block03headline}}</h6>
            </div>
            <div>
                <div class="h1">{{block03header}}<span> {{block03maintitle}}</span></div>
                <div class="educard-heading-line-left"></div>
            </div>
            <div class="row mt-60">
                {{#block03}}
                {{#empty_content}}
                <div class="col-lg-{{block03_grid}} col-md-6">
                    <div class="box">
                        <div class="our-services count-{{count}}">
                            <div class="icons bg-{{count}}"><i class="{{icon}}"></i></div>
                            <h4>{{title}}</h4>
                           <p class="font-size-m">{{#shortentext}} 100, {{{caption}}} {{/shortentext}}</p>
                        </div>
                    </div>
                </div>
                {{/empty_content}}
                {{/block03}}
            </div>
        </div>  
    </div> 
</div>	
